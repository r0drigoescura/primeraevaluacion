# Te muestra la tabla de multiplicar del número que des

def tabla_1():
    ntabla = input("Dime el número de tabla: ")
    for cont in range(1,11):
        print (f"{ntabla}*{cont} = {int(ntabla)*cont}")
        
tabla_1()

# Sin las f-strings

def tabla_1():
    ntabla = input("Dime el número de tabla: ")
    for cont in range(1,11):
        print (str(ntabla)+"*"+str(cont)+" = "+str(int(ntabla)*cont))
tabla_1()
